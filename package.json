{
	"name": "sales-management",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate:pg",
		"custom": "drizzle-kit generate:pg --custom",
		"migrate": "tsx -r dotenv/config ./src/lib/server/drizzle/migrate.ts",
		"push": "drizzle-kit push:pg",
		"seed": "tsx -r dotenv/config ./src/lib/server/drizzle/seed.ts",
		"seeding": "npx vite-node ./src/lib/server/drizzle/seed.ts"
	},
	"devDependencies": {
		"@fast-csv/parse": "^4.3.6",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.30.3",
		"@tailwindcss/forms": "^0.5.7",
		"@trpc/client": "^10.44.1",
		"@trpc/server": "^10.44.1",
		"@types/lodash-es": "^4.17.12",
		"@types/money": "^0.2.3",
		"@types/pg": "^8.10.9",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"bignumber.js": "^9.1.2",
		"buffer": "^6.0.3",
		"currency.js": "^2.0.4",
		"daisyui": "^3.9.4",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"drizzle-zod": "^0.5.1",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lodash-es": "^4.17.21",
		"lucia": "^2.7.5",
		"money": "^0.2.0",
		"pg": "^8.11.3",
		"postcss": "^8.4.32",
		"postgres": "^3.4.3",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"puppeteer": "^21.6.1",
		"superjson": "^1.13.3",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-flatpickr-plus": "^1.0.7",
		"tailwindcss": "^3.3.6",
		"trpc-sveltekit": "^3.5.22",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typescript": "^5.3.3",
		"uuid": "^9.0.1",
		"vite": "^4.5.1",
		"vite-node": "^0.34.7",
		"vitest": "^0.32.4",
		"zod": "^3.22.4"
	}
}
